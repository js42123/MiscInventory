@model IEnumerable<MiscInventory.Models.T115_MISC_TRAN_ARCHV>

@(
    Layout=""
)


    <div class="container">
        <div class="row">
            <div class="col-12 text-center">
                <div class="card contact shadow-sm mb-3 p-1 bg-white rounded">
                    <div class="card-header card-title">
                        <h4 class="title m-0">Archived Transaction Results</h4>
                    </div>
                    <div class="card-body" style="color: black">
                        <div style="overflow: scroll;" id="DivMainContent">
                            <table id="tblSearch" style="font-size: 0.9em;" border="1">
                                <tr class="header">
                                    <th style="font-size: 0.8em">
                                        @Html.DisplayNameFor(model => model.ID_R)
                                    </th>
                                    <th style="font-size: 0.8em">
                                        @Html.DisplayNameFor(model => model.TRANS)
                                    </th>
                                    <th style="font-size: 0.8em">
                                        @Html.DisplayNameFor(model => model.INV_TYPE)
                                    </th>
                                    <th style="font-size: 0.8em">
                                        @Html.DisplayNameFor(model => model.INV_DESC)
                                    </th>
                                    <th style="font-size: 0.8em">
                                        @Html.DisplayNameFor(model => model.BUD)
                                    </th>
                                    <th style="font-size: 0.8em">
                                        @Html.DisplayNameFor(model => model.LOC)
                                    </th>

                                    <th style="font-size: 0.8em">
                                        @Html.DisplayNameFor(model => model.COST)
                                    </th>
                                    <th style="font-size: 0.8em">
                                        @Html.DisplayNameFor(model => model.DEPR)
                                    </th>
                                    <th style="font-size: 0.8em">
                                        @Html.DisplayNameFor(model => model.SUB_TYPE)
                                    </th>
                                    <th style="font-size: 0.8em">
                                        @Html.DisplayNameFor(model => model.REQ_NO)
                                    </th>
                                    <th style="font-size: 0.8em">
                                        @Html.DisplayNameFor(model => model.VCHR_NO)
                                    </th>
                                    <th style="font-size: 0.8em">
                                        @Html.DisplayNameFor(model => model.DOC_DATE)
                                    </th>
                                    <th style="font-size: 0.8em">
                                        @Html.DisplayNameFor(model => model.INV_DATE)
                                    </th>
                                    <th style="font-size: 0.8em">
                                        @Html.DisplayNameFor(model => model.INV_TIME)
                                    </th>
                                    <th style="font-size: 0.8em">
                                        @Html.DisplayNameFor(model => model.FEDID)
                                    </th>
                                    <th style="font-size: 0.8em">
                                        @Html.DisplayNameFor(model => model.TRANS_USERID)
                                    </th>

                                </tr>

                                @foreach (var arch in Model)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => arch.ID_R)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => arch.TRANS)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => arch.INV_TYPE)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => arch.INV_DESC)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => arch.BUD)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => arch.LOC)
                                        </td>

                                        <td>
                                            @Html.DisplayFor(modelItem => arch.COST)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => arch.DEPR)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => arch.SUB_TYPE)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => arch.REQ_NO)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => arch.VCHR_NO)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => arch.DOC_DATE)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => arch.INV_DATE)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => arch.INV_TIME)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => arch.FEDID)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => arch.TRANS_USERID)
                                        </td>
                                    </tr>
                                }

                            </table>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

<script type="text/javascript">

    $(document).ready(function () {
        $("#tblSearch tr:nth-child(even)").addClass("even");
        $("#tblSearch tr:nth-child(odd)").addClass("odd");
    });


</script>



