@model IEnumerable< MiscInventory.Models.T115_MISC_TRANS>
@using MiscInventory.Models;

@using (Html.BeginForm("Transaction","MiscInventory", FormMethod.Post))

{
    <div style="text-align: right">
        <input type="submit" name="Search" value="Search" />
    </div>
    <div class="datagrid"></div>
    <br />
    <div id="DivRoot" style="text-align: left">
        <div style="overflow: hidden;" id="DivHeaderRow">
        </div>
        <div style="overflow: scroll;" onscroll="OnScrollDiv(this)" id="DivMainContent">
            <table id="tblSearch" style="font-size: 0.9em;">
                <tr class="header">
                    <th style="font-size: 0.8em">
                        @Html.DisplayNameFor(model => model.TRANS)
                    </th>
                    <th style="font-size: 0.8em">
                        @Html.DisplayNameFor(model => model.INV_TYPE)
                    </th>
                    <th style="font-size: 0.8em">
                        @Html.DisplayNameFor(model => model.INV_DESC)
                    </th>
                    <th style="font-size: 0.8em">
                        @Html.DisplayNameFor(model => model.BUD)
                    </th>
                    <th style="font-size: 0.8em">
                        @Html.DisplayNameFor(model => model.LOC)
                    </th>
                    <th style="font-size: 0.8em">
                        @Html.DisplayNameFor(model => model.PURCH_YY)
                    </th>
                    <th style="font-size: 0.8em">
                        @Html.DisplayNameFor(model => model.PURCH_MM)
                    </th>
                    <th style="font-size: 0.8em">
                        @Html.DisplayNameFor(model => model.COST)
                    </th>
                    <th style="font-size: 0.8em">
                        @Html.DisplayNameFor(model => model.DEPR)
                    </th>
                    <th style="font-size: 0.8em">
                        @Html.DisplayNameFor(model => model.SUB_TYPE)
                    </th>
                    <th style="font-size: 0.8em">
                        @Html.DisplayNameFor(model => model.REQ_NO)
                    </th>
                    <th style="font-size: 0.8em">
                        @Html.DisplayNameFor(model => model.VCHR_NO)
                    </th>
                    <th style="font-size: 0.8em">
                        @Html.DisplayNameFor(model => model.DOC_DATE)
                    </th>
                    <th style="font-size: 0.8em">
                        @Html.DisplayNameFor(model => model.INV_DATE)
                    </th>
                    <th style="font-size: 0.8em">
                        @Html.DisplayNameFor(model => model.INV_TIME)
                    </th>
                    <th style="font-size: 0.8em">
                        @Html.DisplayNameFor(model => model.FEDID)
                    </th>
                    <th style="font-size: 0.8em">
                        @Html.DisplayNameFor(model => model.TRANS_USERID)
                    </th>

                </tr>

                    @foreach (var transaction in Model)
                {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem =>  transaction.TRANS)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => transaction.INV_TYPE)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => transaction.INV_DESC)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => transaction.BUD)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => transaction.LOC)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => transaction.PURCH_YY)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => transaction.PURCH_MM)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => transaction.COST)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => transaction.DEPR)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => transaction.SUB_TYPE)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => transaction.REQ_NO)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => transaction.VCHR_NO)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => transaction.DOC_DATE)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => transaction.INV_DATE)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => transaction.INV_TIME)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => transaction.FEDID)
                            </td>
                          <td>
                                @Html.DisplayFor(modelItem => transaction.TRANS_USERID)
                            </td>
                        </tr>
                }

                </table>
        </div>
    </div>
    <div id="DivFooterRow" style="overflow: hidden">
    </div>

    <br />
}
 

